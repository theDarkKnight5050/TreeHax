{
  "name": "Newversify",
  "version": "1.0",
  "description": "Stay fully informed with this algorithmic bias checker",

  "incognito": "not_allowed",
  "permissions": [
    "history",
    "storage",
    "tabs"
  ],

  "background": {
    "scripts": ["src/background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {

    },
    "default_title": "Newversify"
  },

  "author": "Ahmed Abdalla",
  "manifest_version": 2
}
